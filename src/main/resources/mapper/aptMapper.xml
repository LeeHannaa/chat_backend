<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aptMapper">
    <select id="findByUserId" parameterType="Long" resultType="Apt">
        select * from APT WHERE user_id = #{userId}
    </select>

    <select id="findAll" resultType="Apt">
        select * from APT
    </select>

    <select id="findById" parameterType="Long" resultType="Apt">
        select * from APT WHERE id = #{id}
    </select>
</mapper>
